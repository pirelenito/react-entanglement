{
  "name": "react-entanglement",
  "version": "1.0.1",
  "description": "Spooky action at a distance",
  "main": "index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --ignore *.spec.js",
    "build:demo": "cross-env NODE_ENV=production sagui build --optimize",
    "dist": "npm run build",
    "format": "sagui format",
    "start": "sagui develop --port 3000",
    "test": "sagui test",
    "test:lint": "sagui test:lint",
    "test:typecheck": "sagui test:typecheck",
    "test:unit": "sagui test:unit --coverage",
    "test:unit:watch": "sagui test:unit --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-entanglement/react-entanglement.git"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "portal"
  ],
  "author": "pirelenito",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-entanglement/react-entanglement/issues"
  },
  "homepage": "https://github.com/react-entanglement/react-entanglement#readme",
  "peerDependencies": {
    "react": "^15.5.4 || 16"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "sagui": "^11.3.0",
    "react": "16",
    "react-dom": "16"
  }
}
